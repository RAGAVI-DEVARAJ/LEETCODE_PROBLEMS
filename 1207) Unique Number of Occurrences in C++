class Solution {
public:
    bool uniqueOccurrences(vector<int>& arr) {
        map<int,int>mpp;
        for(int i=0;i<arr.size();i++){
            mpp[arr[i]]++;
        }
        vector<int>check;
        for(auto it:mpp){
            check.push_back(it.second);
        }
        sort(check.begin(),check.end());
        for(int i=1;i<check.size();i++){
            if(check[i-1]==check[i])return false;
        }
        return true;
    }
};
