class Solution {
public:
    string finalString(string s) {
        string ans;
        for(int i=0;i<s.size();i++){
            if(s[i]=='i'){
                reverse(s.begin(),s.begin()+i);
            }
        }
        for(int j=0;j<s.size();j++){
            if(s[j]!='i'){
                ans+=s[j];
            }
        }
        return ans;
    }
};
